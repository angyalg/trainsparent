<div class="container">
  <div class="row">

      <% @cards.each_slice(4) do |card| %>
      <!-- <div class="row"> -->
        <% card.each do |card| %>
          <div class="col-xs-4 col-sm-3 card-wrapper">
          <div class="memory-card">
            <%= image_tag (card[0]) %>
            <p> <%= card[1] %></p>
          </div>
          </div>
        <% end %>
      <!-- </div> -->
      <% end %>

    </div>
  </div>

</div>

